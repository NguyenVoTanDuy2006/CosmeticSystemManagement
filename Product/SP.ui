#formCard QComboBox {
  min-height: 40px;
  background-color: #FFFFFF;        /* override lại vì dòng QWidget ở trên */
  color: #0B2B12;
  border: 2px solid #64C57F;
  border-radius: 16px;
  padding: 8px 12px;
}
#formCard QLineEdit:focus,
#formCard QComboBox:focus {
  border: 2px solid #2FB563;
}

/* MÃ SP nếu readOnly: nền hơi nhạt để phân tầng */
