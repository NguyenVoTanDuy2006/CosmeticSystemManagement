               <string notr="true">#widgetFilter {
  /* gradient rất nhẹ: trắng -&gt; mint xanh lam nhạt */
  background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
                              stop:0  #FFFFFF,
                              stop:1  #F4FBFF);  /* có thể thử #F3FFF8 để ngả xanh lá hơn */
  border: 10px solid #BFD9FF;    /* viền xanh lam nhạt */
  border-radius: 14px;
  /* khoảng thở bên trong panel (nếu muốn): 
     padding: 8px;  // optional */
}

/* ====== HEADER ====== */
#header {
  padding: 10px 14px;
  border-bottom: 2px solid #D6E8FF;
}
#lblTitle {
  color: #0A2FA0;                  /* navy */
  font-weight: 800;
  font-size: 30px;                 /* to như mẫu */
  letter-spacing: .3px;
}

/* nút đóng (dùng kí tự ✕ hoặc icon) */
QPushButton#btnClose {
  min-width: 38px; min-height: 38px;
  border-radius: 19px;
  background: #FFFFFF;

  color: #0033A0;
  font-weight: 800;
  font-size: 22px;
}
QPushButton#btnClose:hover  { background: #EFFFF7; }
QPushButton#btnClose:pressed{ background: #DFF7EB; }

/* ====== TIÊU ĐỀ NHÓM (Hạn sử dụng / Công dụng / Thương Hiệu) ====== */
/* đặt property section=true cho các QLabel: lblHSD, lblType, lblBrand */
#uiFilterSP QLabel {
  color: #0A2FA0;
  font-weight: 800;
  font-size: 25px;
  margin: 14px 0 6px 0;
}

/* ====== CHECKBOX (HSD + công dụng) ====== */
#uiFilterSP QCheckBox {
  color: #1B1399;                   /* xanh đậm hơi tím như chữ mẫu */
  font-size: 20px;
  font-weight: 800;
  spacing: 12px;                    /* khoảng cách indicator - text */
  padding: 6px 0;
}
#uiFilterSP QCheckBox::indicator {
  width: 30px; height: 30px;        /* to như hình */
  border: 4px solid #2EC08A;        /* viền xanh lá nhạt */
  border-radius: 9px;               /* ô vuông bo góc */
  background: #FFFFFF;
  margin-right: 8px;
}
#uiFilterSP QCheckBox::indicator:hover  { background: #EEFFF6; }
#uiFilterSP QCheckBox::indicator:checked{
  background: #2EC08A;              /* khi tick, fill xanh lá */
  border-color: #2EC08A;
}

/* ====== SCROLL CÔNG DỤNG ====== */
QScrollArea#scrollType {
  border: 2px solid #CFEBD2;
  border-radius: 12px;
}
QScrollArea#scrollType &gt; QWidget { background: transparent; }
QScrollArea#scrollType &gt; QWidget &gt; QWidget { background: transparent; }

/* thanh cuộn dọc: track xanh mint, handle xám tròn, có nút đầu/cuối */
QScrollBar:vertical {
  background: #E8FAEE;             /* track mint nhạt */
  width: 22px;
  margin: 6px 4px;
  border-radius: 11px;
}
QScrollBar::handle:vertical {
  background: #A9A9A9;              /* tay nắm xám */
  border-radius: 10px;
  min-height: 40px;
}
QScrollBar::handle:vertical:hover { background: #8F8F8F; }

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
  background: #E0E0E0;
  border-radius: 6px;
  height: 22px;
  subcontrol-origin: margin;
}
QScrollBar::sub-line:vertical { subcontrol-position: top;    }
QScrollBar::add-line:vertical { subcontrol-position: bottom; }
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical { background: none; }

/* ====== Ô NHẬP THƯƠNG HIỆU ====== */
QLineEdit#brandEdit {
  border: 3px solid #2B6FD3;        /* viền xanh dương như ô tìm kiếm */
  border-radius: 16px;
  padding: 10px 14px;
  font-size: 15px;
  color: #0A2FA0;
  background: #FFFFFF;
}
QLineEdit#brandEdit:focus { border-color: #0A2FA0; }

/* ====== LINK “Xem 0 kết quả” ====== */
/* dùng QPushButton phẳng làm link: objectName=btnSee */
QPushButton#btnApply {
